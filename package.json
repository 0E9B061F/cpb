{
  "name": "cpb",
  "version": "2.0.5",
  "private": true,
  "scripts": {
    "build": "env NODE_ENV=production rollup -c",
    "build:dev": "env NODE_ENV=development rollup -c",
    "postbuild": "node server/build/build.js",
    "dev": "env NODE_ENV=development rollup -c -w",
    "app": "env NODE_ENV=production server/bin/start.js",
    "dump:dev": "rm -f cpb.dev.sqlite",
    "dump:test": "rm -f cpb.test.sqlite",
    "dump": "rm -f cpb.prod.sqlite",
    "mig:dev": "env NODE_ENV=development npx sequelize-cli db:migrate --debug",
    "mig": "env NODE_ENV=production npx sequelize-cli db:migrate",
    "sync": "server/bin/sync.js",
    "seed": "env NODE_ENV=production npx sequelize-cli db:seed --seed 20220126035836-root-user --seed 20220126035855-default-page --debug",
    "seed:dev": "env NODE_ENV=development npx sequelize-cli db:seed:all --debug",
    "refresh:dev": "run-s dump:dev mig:dev seed:dev build:dev",
    "refresh": "run-s dump mig seed build",
    "test": "jest"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-replace": "^4.0.0",
    "jest": "^28.1.1",
    "lorem-ipsum": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sequelize-cli": "^6.4.1",
    "supertest": "^6.2.3",
    "svelte": "^3.48.0"
  },
  "dependencies": {
    "@rollup/plugin-json": "^4.1.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.1",
    "compression": "^1.7.4",
    "connect-session-sequelize": "^7.1.2",
    "cors": "^2.8.5",
    "date-and-time": "^2.1.0",
    "express": "^4.17.2",
    "express-session": "^1.17.2",
    "html-to-text": "^8.1.0",
    "html-word-count": "^2.0.0",
    "i-ching": "^0.3.5",
    "js-cookie": "^3.0.1",
    "marked": "^4.0.12",
    "multer": "^1.4.5-lts.1",
    "mustache": "^4.2.0",
    "qrcode": "^1.5.0",
    "rimraf": "^3.0.2",
    "sequelize": "^6.12.5",
    "serve-static": "^1.15.0",
    "sharp": "^0.30.6",
    "sirv": "^2.0.0",
    "sirv-cli": "^1.0.0",
    "sqlite3": "^5.0.2",
    "svelte-markdown": "^0.2.1",
    "svelte-routing": "^1.6.0",
    "uuid": "^8.3.2"
  }
}
